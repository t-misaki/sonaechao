<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <ImageView
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:id="@+id/hijoview"
        android:src="@drawable/graph0"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="73dp" />

    <ImageView
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:id="@+id/bichikuview"
        android:src="@drawable/rightgraph0"
        android:layout_alignTop="@+id/hijoview"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:text="非常食"
        android:textSize="30dp"
        android:layout_above="@+id/bichikuview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:text="備蓄品"
        android:textSize="30dp"
        android:layout_alignTop="@+id/editText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:id="@+id/scrollView2"
        android:layout_gravity="center_horizontal"
        android:layout_above="@+id/HomeButton"
        android:layout_alignRight="@+id/SettingButton"
        android:layout_alignEnd="@+id/SettingButton">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/bichikupercent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/linearLayout"
            android:background="#fffff0bb">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/retorutog"
                android:textSize="10dp"
                android:clickable="true"
                android:layout_below="@+id/test"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kandume"
                android:textSize="10dp"
                android:layout_below="@+id/retorutog"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kanmen"
                android:textSize="10dp"
                android:layout_below="@+id/kandume"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kanpan"
                android:textSize="10dp"
                android:layout_below="@+id/kanmen"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kandume2"
                android:textSize="10dp"
                android:layout_below="@+id/kanpan"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/retoruto"
                android:textSize="10dp"
                android:layout_below="@+id/kandume2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/freaze"
                android:textSize="10dp"
                android:layout_below="@+id/retoruto"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kasi"
                android:textSize="10dp"
                android:layout_below="@+id/freaze"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/karori"
                android:textSize="10dp"
                android:layout_below="@+id/kasi"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/rinyu"
                android:textSize="10dp"
                android:layout_below="@+id/karori"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/konamilk"
                android:textSize="10dp"
                android:layout_below="@+id/rinyu"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/mizu"
                android:textSize="10dp"
                android:layout_below="@+id/konamilk"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
        </LinearLayout>
    </ScrollView>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/hijolasttext"
        android:text="最終入力日"
        android:textSize="10dp"
        android:layout_alignBottom="@+id/hijoview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bichikulasttext"
        android:text="最終入力日"
        android:textSize="10dp"
        android:layout_alignBottom="@+id/bichikuview"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="広告"
        android:id="@+id/textView25"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff9aff98" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/HomeButton"
        android:layout_above="@+id/textView25"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/home" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/HijoButton"
        android:layout_above="@+id/textView25"
        android:layout_toRightOf="@+id/HomeButton"
        android:layout_toEndOf="@+id/HomeButton"
        android:src="@drawable/hijo" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/SettingButton"
        android:layout_alignBottom="@+id/HijoButton"
        android:layout_alignRight="@+id/bichikulasttext"
        android:layout_alignEnd="@+id/bichikulasttext"
        android:src="@drawable/setting" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/BichikuButton"
        android:layout_alignBottom="@+id/SettingButton"
        android:layout_toLeftOf="@+id/SettingButton"
        android:layout_toStartOf="@+id/SettingButton"
        android:src="@drawable/bichiku" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="非常食:0％"
        android:id="@+id/hijopercent"
        android:layout_below="@+id/hijoview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="備蓄品:0％"
        android:id="@+id/bichikupercent"
        android:layout_below="@+id/bichikuview"
        android:layout_alignRight="@+id/bichikuview"
        android:layout_alignEnd="@+id/bichikuview"
        android:textSize="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="要チェック"
        android:id="@+id/textView"
        android:layout_above="@+id/scrollView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
