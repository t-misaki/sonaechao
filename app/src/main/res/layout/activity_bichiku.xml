<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.misakitatsuya.hello_world.bichiku">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/button12"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:id="@+id/button13"
        android:layout_alignTop="@+id/button12"
        android:layout_alignRight="@+id/pop7"
        android:layout_alignEnd="@+id/pop7" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:id="@+id/button14"
        android:layout_alignTop="@+id/button13"
        android:layout_alignLeft="@+id/pop8"
        android:layout_alignStart="@+id/pop8" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="設定"
        android:id="@+id/button15"
        android:layout_alignTop="@+id/button14"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop1"
        android:src="@drawable/gas"
        android:layout_marginTop="32dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/pop3"
        android:layout_alignStart="@+id/pop3" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop2"
        android:src="@drawable/matti"
        android:layout_alignTop="@+id/pop1"
        android:layout_toLeftOf="@+id/button15"
        android:layout_toStartOf="@+id/button15" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop3"
        android:src="@drawable/bombe"
        android:layout_below="@+id/pop1"
        android:layout_alignLeft="@+id/pop5"
        android:layout_alignStart="@+id/pop5" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop4"
        android:src="@drawable/almi"
        android:layout_alignTop="@+id/pop3"
        android:layout_alignLeft="@+id/pop2"
        android:layout_alignStart="@+id/pop2" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop5"
        android:src="@drawable/gunnte"
        android:layout_below="@+id/pop3"
        android:layout_toRightOf="@+id/button12"
        android:layout_toEndOf="@+id/button12" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop6"
        android:src="@drawable/hue"
        android:layout_below="@+id/pop4"
        android:layout_alignLeft="@+id/pop4"
        android:layout_alignStart="@+id/pop4" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop7"
        android:src="@drawable/sitagi"
        android:layout_below="@+id/pop5"
        android:layout_alignLeft="@+id/pop5"
        android:layout_alignStart="@+id/pop5" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/pop8"
        android:src="@drawable/thissyu"
        android:layout_below="@+id/pop6"
        android:layout_alignLeft="@+id/pop6"
        android:layout_alignStart="@+id/pop6" />

</RelativeLayout>
